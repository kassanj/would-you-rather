    <div class="row">

      <div class="col-sm-6">
        <div class="card">
          <div class="card-header">
            <strong>Calendars</strong>
            <small></small>
          </div>
          <div class="card-block">
            <%= render 'inventory_calendar' %>
          </div>
        </div>
      </div>

     	<div class="col-sm-6">
				<div class="card">
				  <div class="card-header">
  					<strong>New Package</strong>
  					<small></small>
					</div>
					<div class="card-block">
            <%= render 'client_form' %>
	        </div>
        </div>

        <div class="card">
          <div class="card-header">
            <strong>Price Summary</strong>
            <small></small>
          </div>
          <div class="card-block">
            <%= render 'price_summary_table' %>
          </div>
        </div>
      </div>

    </div>
	</div>
</div>

<%= javascript_include_tag 'client-datepicker.js' %>
